// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PRODUCTS {
  id    Int      @id @default(autoincrement())
  brand String
  price Float
  color String
  name  String
  image String
  date  DateTime
}
